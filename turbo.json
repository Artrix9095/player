{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "analyze": {
      "outputs": ["analyze.json", "vscode.html-data.json"]
    },
    "build": {
      "dependsOn": ["^analyze", "^build", "clean"],
      "outputs": ["cdn/**", "dist/**", "local/**", ".vercel/**", "styles/player/themes/default.css"]
    },
    "clean": {
      "cache": false
    },
    "types": {
      "dependsOn": ["^types", "clean"],
      "outputs": ["dist/types/**", "*.d.ts"]
    },
    "format": {
      "inputs": ["src/**"]
    },
    "test": {
      "inputs": ["src/**"]
    }
  }
}
