{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "analyze": {
      "dependsOn": ["^types"],
      "outputs": ["analyze.json", "vscode.html-data.json", "vue.d.ts", "svelte.d.ts", "solid.d.ts"]
    },
    "build": {
      "dependsOn": ["^analyze", "^build", "clean"],
      "outputs": [
        "cdn/**",
        "dist/**",
        "local/**",
        ".vercel/**",
        "player/styles/default/theme.css",
        "tailwind.cjs",
        "tailwind.d.cts"
      ]
    },
    "build:vercel": {
      "dependsOn": ["^analyze", "^build", "clean"],
      "outputs": [".vercel/**"]
    },
    "clean": {
      "cache": false
    },
    "types": {
      "dependsOn": ["^types", "clean"],
      "outputs": ["dist/types/**", "*.d.ts"]
    },
    "format": {
      "inputs": ["src/**"]
    },
    "test": {
      "inputs": ["src/**"]
    }
  }
}
