{
  "name": "@vidstack/react",
  "version": "0.6.13",
  "description": "UI component library for building high-quality, accessible video and audio experiences on the web.",
  "license": "MIT",
  "type": "module",
  "types": "index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "files": [
    "*.d.ts",
    "dist/"
  ],
  "scripts": {
    "dev": "pnpm clean && pnpm prebuild && rollup -c -w",
    "prebuild": "node .scripts/prebuild.js",
    "build": "pnpm prebuild && rollup -c",
    "types": "tsc -p tsconfig.build.json && rollup -c --config-types",
    "clean": "rimraf dist styles",
    "format": "prettier src --write --loglevel warn"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0"
  },
  "dependencies": {
    "media-captions": "0.0.18"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/fs-extra": "^11.0.1",
    "@types/react": "^18.0.0",
    "esbuild": "^0.16.17",
    "fs-extra": "^11.0.0",
    "maverick.js": "0.38.2",
    "media-icons": "0.10.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^3.25.1",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.0",
    "vidstack": "workspace:*"
  },
  "contributors": [
    "Rahim Alwer <rahim.alwer@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidstack/vidstack.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/vidstack/vidstack/issues"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "node": "./dist/server/vidstack.js",
      "deno": "./dist/server/vidstack.js",
      "development": "./dist/dev/vidstack.js",
      "default": "./dist/prod/vidstack.js"
    },
    "./icons": {
      "types": "./icons.d.ts",
      "default": "./dist/prod/vidstack-icons.js"
    },
    "./player/default-skin": {
      "types": "./player/default-skin.d.ts",
      "node": "./dist/server/vidstack-player-skin.js",
      "deno": "./dist/server/vidstack-player-skin.js",
      "development": "./dist/dev/vidstack-player-skin.js",
      "default": "./dist/prod/vidstack-player-skin.js"
    },
    "./dist/types/*": "./dist/types/*",
    "./styles/*": "./styles/*",
    "./package.json": "./package.json",
    "./tailwind.cjs": {
      "types": "./tailwind.d.cts",
      "default": "./tailwind.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "accessible",
    "analytics",
    "audio",
    "customizable",
    "dailymotion",
    "elements",
    "extensible",
    "file",
    "framework-agnostic",
    "headless",
    "hls",
    "html5",
    "jwplayer",
    "media",
    "plyr",
    "player",
    "react",
    "spec-compliant",
    "universal",
    "uploader",
    "vds",
    "video",
    "videojs",
    "vidstack",
    "vime",
    "vimeo",
    "web-component",
    "tailwind",
    "universal",
    "youtube"
  ]
}
